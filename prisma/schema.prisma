generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model inmate{
  name String
  pid  String @id @default(uuid())
  dob  DateTime
  gender String
  village String
  gewog String
  dzongkhag String
  martialStatus String
  nationality String
  height Int
  weight Int
  complexion String
  hairColor String
  eyeColor String
  crimes String
  sentence Int
  startdate DateTime
  enddate DateTime
  prison String
  cell String
}

model visitor {
  name String
  visitNo String @id @default(uuid())
  pname String
  cid String
  contact String
  date DateTime @default(now())
}